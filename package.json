{
  "name": "medius",
  "productName": "Medius",
  "version": "1.0.2",
  "description": "Brinque e altere os dados com Medius",
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/joloran/jolora.git"
  },
  "scripts": {
    "start": "dotenv -e .env -- electron-forge start",
    "package:forge": "dotenv -e .env -- electron-forge package",
    "make:forge": "dotenv -e .env -- electron-forge make",
    "package": "dotenv -e .env -- electron-builder --dir",
    "make": "dotenv -e .env -- npm run package:forge && npm run fix-webpack && electron-builder",
    "pre-release": "dotenv -e .env -- npm run package:forge && npm run fix-webpack",
    "release": "dotenv -e .env -- npm run pre-release && electron-builder --publish always",
    "lint": "eslint --ext .ts,.tsx .",
    "fix-webpack": "node fix-webpack-output.js"
  },
  "devDependencies": {
